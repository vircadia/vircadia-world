{
    "name": "vircadia-world-cli",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "server:container:run-command": "bun vircadia.world.cli.ts server:container:run-command",
        "server:container:rebuild-all": "bun run server:container:run-command down -v && bun run server:container:run-command up --build postgres -d && bun run server:container:postgres:health && bun run server:container:postgres:migrate && bun run server:container:postgres:seed && bun run server:container:run-command up --build world_api_manager -d && bun run server:container:run-command up --build world_tick_manager -d && bun run server:container:run-command up --build pgweb -d",

        "server:container:postgres:health": "bun vircadia.world.cli.ts server:container:postgres:health",
        "server:container:postgres:migrate": "bun vircadia.world.cli.ts server:container:postgres:migrate",
        "server:container:postgres:wipe": "bun vircadia.world.cli.ts server:container:postgres:wipe",
        "server:container:postgres:connection-string": "bun vircadia.world.cli.ts server:container:postgres:connection-string",
        "server:container:postgres:system-token": "bun vircadia.world.cli.ts server:container:postgres:system-token",
        "server:container:postgres:system-token:invalidate-all": "bun vircadia.world.cli.ts server:container:postgres:system-token:invalidate-all",
        "server:container:postgres:seed": "bun vircadia.world.cli.ts server:container:postgres:seed",

        "server:container:world-api-manager:health": "bun vircadia.world.cli.ts server:container:world-api-manager:health",

        "server:container:world-tick-manager:health": "bun vircadia.world.cli.ts server:container:world-tick-manager:health",

        "server:container:pgweb:health": "bun vircadia.world.cli.ts server:container:pgweb:health",
        "server:container:pgweb:access-command": "bun vircadia.world.cli.ts server:container:pgweb:access-command",

        "client:container:run-command": "bun vircadia.world.cli.ts client:container:run-command",
        "client:container:rebuild-all": "bun run client:container:run-command down -v && bun run client:container:run-command up --build -d",

        "client:container:web_babylon_js_prod:health": "bun vircadia.world.cli.ts client:container:web_babylon_js_prod:health",
        "client:container:web_babylon_js_dev:health": "bun vircadia.world.cli.ts client:container:web_babylon_js_dev:health"
    },
    "dependencies": {
        "jsonwebtoken": "^9.0.2",
        "postgres": "^3.4.5"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/jsonwebtoken": "latest",
        "@types/node": "^22.13.9"
    },
    "engines": {
        "bun": ">=1.2.4"
    },
    "module": "index.ts",
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
